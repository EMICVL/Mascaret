<?xml version="1.0" encoding="UTF-8"?>
<Widgets>

  <!--
     List of signal
     one signal is a button
    -->
  <Widget ID="window" class="AwArray" tooltip="SignalsWindow" mode="vert" space="10" visible="FALSE">
    <Appearance stock="Bkg128x128" bkgR="0.5" bkgB="1" bkgG="0.5"/>
    <Widget ID="classLabel" class="AwLabel" justify="left" sensible="FALSE" expand="TRUE">
    </Widget>
        
    <Widget ID="signalsArray" class="AwArray" mode="vert" sensible="FALSE" expand="TRUE">
      <Appearance stock="Bkg128x128"/>
      <Widget class="AwLabel" justify="left" sensible="FALSE" expand="TRUE">
      <Text>Signals :</Text>
      </Widget>
    </Widget>
    
    <Widget class="AwArray" mode="horiz" sensible="FALSE" expand="TRUE">
      <Appearance stock="Bkg128x128"/>
      <Widget ID="cancelButton" class="AwButton" tooltip="Cancel" labelPosition="bottom">
	<Label><Text>Cancel</Text></Label>
      </Widget>      

    </Widget>

  </Widget>

<Widget ID="parametersWindow" class="AwArray" tooltip="ParametersWindow" mode="vert" space="10" visible="FALSE">
    <Appearance stock="Bkg128x128" bkgR="0.5" bkgB="1" bkgG="0.5"/>
    <Widget ID="classLabelPara" class="AwLabel" justify="left" sensible="FALSE" expand="TRUE">
    </Widget>
    <Widget ID="parametersArray" class="AwArray" mode="vert" sensible="FALSE" expand="TRUE">
      <Appearance stock="Bkg128x128"/>
      <Widget class="AwLabel" justify="left" sensible="FALSE" expand="TRUE">
      <Text>Parameters :</Text>
      </Widget>
    </Widget>

    <Widget class="AwArray" mode="horiz" sensible="FALSE" expand="TRUE">
      <Appearance stock="Bkg128x128"/>

      <Widget ID="executeButton" class="AwButton" tooltip="Execute" labelPosition="bottom">
        <Label><Text>Execute</Text></Label>
      </Widget>

      <Widget ID="cancelParaButton" class="AwButton" tooltip="Cancel" labelPosition="bottom">
        <Label><Text>Cancel</Text></Label>
      </Widget>

    </Widget>

  </Widget>


</Widgets>
